{
    "name": "kanekoelastic/codenberg-php",
    "type": "library",
    "description": "PHP Wrapper for Codenberg API",
    "keywords": [
        "php",
        "sdk",
        "api",
        "codenberg"
    ],
    "homepage": "https://github.com/kanekoelastic/phpcodenberg",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "kanekoelastic",
            "homepage": "https://kanekoelastic.org"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.2.*",
        "friendsofphp/php-cs-fixer": "2.9.*",
        "phpunit/phpunit": "5.7.*",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Kanekoelastic\\PhpCodenberg\\" : "lib/",
            "Kanekoelastic\\PhpCodenberg\\Api\\" : "lib/Api/",
            "Kanekoelastic\\PhpCodenberg\\Model\\" : "lib/Model/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kanekoelastic\\PhpCodenberg\\" : "tests/"
        }
    }
}
